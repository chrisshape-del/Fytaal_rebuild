{
    "name": "fytaal-rebuild",
    "compatibility_date": "2024-11-01",
    "main": "src/worker.js",
    "assets": {
        "directory": "./dist",
        "binding": "ASSETS"
    },
    "d1_databases": [
        {
            "binding": "DB",
            "database_name": "fytaal-db",
            "database_id": "LOCALLY_MANAGED"
        }
    ],
    "r2_buckets": [
        {
            "binding": "MEDIA",
            "bucket_name": "fytaal-media"
        }
    ],
    "routes": [
        {
            "pattern": "/*",
            "zone_name": "fytaal.com"
        }
    ]
}